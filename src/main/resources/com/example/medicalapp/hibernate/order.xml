<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.example.medicalapp.domain.Order"
           table="Orders">
        <id name="id" column="orderid" type="integer">
            <!--<generator class="increment"/>-->
        </id>
        <property
                name="date"
                column="orderDate"
                type="string"/>
        <property
                name="description"
                column="description"
                type="string"/>

        <property
                name="userid"
                column="userid"
                type="integer"/>


    </class>
</hibernate-mapping>
