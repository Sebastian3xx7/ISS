<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.example.medicalapp.domain.OrderLine"
           table="OrderLines">
        <id name="id" column="id" type="integer">
            <!--<generator class="increment"/>-->
        </id>
        <property
                name="medicine"
                column="medicine"
                type="string"/>
        <property
                name="quantity"
                column="quantity"
                type="integer"/>
        <property
                name="orderid"
                column="orderid"
                type="integer"/>
    </class>
</hibernate-mapping>
