<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.example.medicalapp.domain.Pharmacist"
           table="Pharmacists">
        <id name="id" column="id" type="integer">
            <!--<generator class="increment"/>-->
        </id>
        <property
                name="username"
                column="username"
                type = "string"/>
        <property
                name="password"
                column="password"
                type = "string"/>
        <property
                name="idPharmacy"
                column="idPharmacy"
                type="java.lang.Integer"/>
    </class>
</hibernate-mapping>